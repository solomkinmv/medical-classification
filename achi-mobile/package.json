{
  "name": "achi-mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "deploy:ios": "eas build --platform ios --profile production --local --non-interactive && IPA=$(ls -t build-*.ipa | head -1) && eas submit --platform ios --path ./$IPA --non-interactive && rm -f $IPA",
    "lint": "expo lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,json}\""
  },
  "dependencies": {
    "@expo/ui": "~0.2.0-beta.7",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "expo": "~54.0.33",
    "expo-blur": "~15.0.6",
    "expo-constants": "~18.0.8",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.9",
    "expo-glass-effect": "~0.1.4",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.9",
    "expo-linking": "~8.0.7",
    "expo-localization": "~17.0.8",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.7",
    "expo-system-ui": "~6.0.7",
    "nativewind": "^4.1.0",
